modules = ["python-3.11", "python3", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["postgresql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DeckForge Bot"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Web Admin Portal"

[[workflows.workflow]]
name = "DeckForge Bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python bot.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Web Admin Portal"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn web.main:app --host 0.0.0.0 --port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

